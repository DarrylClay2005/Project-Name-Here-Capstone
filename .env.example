# Fishial.AI API Configuration
# Copy this file to .env and fill in your actual API credentials
# Get your API keys from https://portal.fishial.ai -> About -> For Developers

# Your API Key ID (from Fishial.AI portal)
VITE_FISHIAL_API_KEY=your_api_key_here

# Your API Secret Key (from Fishial.AI portal)
VITE_FISHIAL_API_SECRET=your_secret_key_here

# API Endpoints (these should not change)
VITE_FISHIAL_AUTH_URL=https://api-users.fishial.ai/v1/auth/token
VITE_FISHIAL_API_BASE_URL=https://api.fishial.ai/v1
